{
  "extends": [
    "config:base",
    ":maintainLockFilesWeekly",
    ":widenPeerDependencies",
    ":prHourlyLimit4",
    ":rebaseStalePrs",
    ":prNotPending"
  ],
  "packageRules": [
    {
      "packagePatterns": "*",
      "groupName": "All Dev",
      "depTypeList": [
        "devDependencies"
      ],
      "circleci": {
        "enabled": true,
        "schedule": [
          "after 9pm on sunday"
        ]
      }
    },
    {
      "packagePatterns": [
        "eslint"
      ],
      "groupName": "eslint"
    },
    {
      "packagePatterns": [
        "semantic-release"
      ],
      "groupName": "semantic-release"
    }
  ],
  "circleci": {
    "enabled": false
  }
}
